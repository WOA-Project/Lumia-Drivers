[Version]
Signature   = "$WINDOWS NT$"
Class       = System
ClassGuid   = {4d36e97d-e325-11ce-bfc1-08002be10318}
Provider    = %COMP%
DriverVer   = 04/15/2015,1.0.2074.0000
CatalogFile = qcmipibif8612.cat
PnpLockDown = 1

[SourceDisksNames]
3426 = windows cd

[SourceDisksFiles]
qcmipibif8612.sys = 3426

;12 == Windows\System32\Drivers
[DestinationDirs]
DefaultDestDir = 12

[ControlFlags]
ExcludeFromSelect = *

;*****************************************
; qcmipibif8612 client driver Install Section
;*****************************************

[Manufacturer]
%COMP% = Standard,NTARM

[Standard.NTARM]
%MipiBif.DeviceDesc% = MipiBif_Inst,ACPI\

[MipiBif_Inst.NT]
Copyfiles   = MipiBifCopyFiles
AddProperty = WDTFSOCDeviceCategory

[MipiBifCopyFiles]
qcmipibif8612.sys, , , 0x100,

[MipiBif_Inst.NT.Services]
AddService = qcmipibif,%SPSVCINST_ASSOCSERVICE%,MipiBif_Service_Inst

[MipiBif_Service_Inst]
DisplayName   = %MipiBif.SvcDesc%
ServiceType   = %SERVICE_KERNEL_DRIVER%
StartType     = %SERVICE_DEMAND_START%
ErrorControl  = %SERVICE_ERROR_NORMAL%
ServiceBinary = %12%\qcmipibif8612.sys

[Strings]
;Localizable Strings
COMP                   = "Qualcomm"
MipiBif.SvcDesc        = "Qualcomm MipiBif WDF Driver"
MipiBif.DeviceDesc     = "Qualcomm MipiBif WDF Driver"

;Non-Localizable Strings
SPSVCINST_TAGTOFRONT   = 0x00000003
SPSVCINST_ASSOCSERVICE = 0x00000002
SERVICE_KERNEL_DRIVER  = 1
SERVICE_BOOT_START     = 0
SERVICE_SYSTEM_START   = 1
SERVICE_DEMAND_START   = 3
SERVICE_ERROR_NORMAL   = 1
SERVICE_ERROR_IGNORE   = 0
SERVICE_ERROR_CRITICAL = 3
REG_EXPAND_SZ          = 0x00020000
REG_DWORD              = 0x00010001
REG_SZ                 = 0x00000000





